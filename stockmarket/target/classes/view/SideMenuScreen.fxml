<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.materialdesignicons.*?>
<?import com.jfoenix.controls.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stockmarket.controller.SideMenuController">
   <children>
      <VBox prefHeight="720.0" prefWidth="200.0" style="-fx-background-color: #203656;">
         <children>
            <Label prefWidth="180.0" text="stockMarket">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </VBox.margin>  
               <padding>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </padding>
               <font>
                  <Font size="30.0" />
               </font>
               <textFill>
                  <LinearGradient endX="0.3786407766990291" endY="0.5145631067961165" startX="0.5048543689320388" startY="0.5">
                     <stops>
                        <Stop color="#08e0d2" />
                        <Stop color="#d3cbcb" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </textFill>
            </Label>
            <Label alignment="CENTER_RIGHT" prefWidth="180.0" text="simulator" textFill="#d3cbcb">
               <font>
                  <Font size="18.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="14.0" left="10.0" right="10.0" />
               </VBox.margin>
            </Label>
            <JFXButton alignment="BASELINE_LEFT" buttonType="RAISED" onAction="#showOverview" prefHeight="116.0" prefWidth="180.0" ripplerFill="#04ffee" style="-fx-background-color: #182942;" text="%overview" textFill="#d3cbcb">
               <VBox.margin>
                  <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <MaterialDesignIconView fill="#d3cbcb" glyphName="CHART_LINE" size="36" />
               </graphic>
               <padding>
                  <Insets left="44.0" />
               </padding>
            </JFXButton>
            <JFXButton alignment="BASELINE_LEFT" buttonType="RAISED" onAction="#showAdd" prefHeight="116.0" prefWidth="180.0" ripplerFill="#08e0d2" style="-fx-background-color: #182942;" text="%add" textFill="#d3cbcb">
               <VBox.margin>
                  <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <MaterialDesignIconView fill="#d3cbcb" glyphName="PLUS" size="36" />
               </graphic>
               <padding>
                  <Insets left="44.0" />
               </padding>
            </JFXButton>
            <JFXButton alignment="BASELINE_LEFT" buttonType="RAISED" onAction="#showSimulation" prefHeight="116.0" prefWidth="180.0" ripplerFill="#08e0d2" style="-fx-background-color: #182942;" text="%simulation" textFill="#d3cbcb">
               <VBox.margin>
                  <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <MaterialDesignIconView fill="#d3cbcb" glyphName="ROTATE_3D" size="36" />
               </graphic>
               <padding>
                  <Insets left="44.0" />
               </padding>
            </JFXButton>
            <JFXButton alignment="BASELINE_LEFT" buttonType="RAISED" onAction="#showSettings" prefHeight="116.0" prefWidth="180.0" ripplerFill="#08e0d2" style="-fx-background-color: #182942;" text="%settings" textFill="#d3cbcb">
               <VBox.margin>
                  <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <MaterialDesignIconView fill="#d3cbcb" glyphName="SETTINGS" size="36" />
               </graphic>
               <padding>
                  <Insets left="44.0" />
               </padding>
            </JFXButton>
            <JFXButton alignment="BASELINE_LEFT" buttonType="RAISED" onAction="#showHelp" prefHeight="116.0" prefWidth="180.0" ripplerFill="#08e0d2" style="-fx-background-color: #182942;" text="%help" textFill="#d3cbcb">
               <VBox.margin>
                  <Insets bottom="4.0" left="8.0" right="8.0" top="4.0" />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <graphic>
                  <MaterialDesignIconView fill="#d3cbcb" glyphName="STACKOVERFLOW" size="36" />
               </graphic>
               <padding>
                  <Insets left="44.0" />
               </padding>
            </JFXButton>
         </children>
      </VBox>
   </children>
</StackPane>
